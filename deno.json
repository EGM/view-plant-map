{
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"]
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.4",
    "@vitejs/plugin-vue": "npm:@vitejs/plugin-vue@^5.2.1",
    "autoprefixer": "npm:autoprefixer@^10.4.20",
    "ol": "npm:ol@^10.4.0",
    "ol-contextmenu": "npm:ol-contextmenu@^5.5.0",
    "ol-ext": "npm:ol-ext@^4.0.26",
    "postcss": "npm:postcss@^8.5.1",
    "tailwindcss": "npm:tailwindcss@^4.0.4",
    "vite": "npm:vite@^6.0.1",
    "vue": "npm:vue@^3.5.13",
    "vue3-openlayers": "npm:vue3-openlayers@^11.3.0"
  }
}
